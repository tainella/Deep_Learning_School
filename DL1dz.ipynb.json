    "class Neuron:\n",
    "\n",
    "    def __init__(self, w, f = lambda x: x):\n",
    "        self.w = w\n",
    "        self.f = f\n",
    "        self._x = None\n",
    "\n",
    "    def forward(self, x):\n",
    "        s = 0\n",
    "        self._x = x\n",
    "        for i, _ in enumerate(self.w):\n",
    "            s += self.w[i] * x[i]\n",
    "        return self.f(s)\n",
    "\n",
    "    def backlog(self):\n",
    "        return self._x\n"
   ]
